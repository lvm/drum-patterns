-- Roger Hawkins
-- Respect yourself
-- 1971

bps (111/120)

drums $
  stack[
    perc "[x ~ ~ ~][x ~ ~ ~][x ~ ~ ~][x ~ ~ ~]" # perc "bd",
    slowcat[
      perc "[~ ~ ~ ~][x ~ ~ ~][~ ~ x ~][x ~ ~ ~]" # perc "sn",
      perc "[~ ~ ~ ~][x ~ ~ ~][x ~ x ~][x ~ ~ ~]" # perc "sn"
      ] # nudge "0.05",
    perc $ preplace (1,1) ("x(8,16)" :: Pattern String) "ch"
    ]


