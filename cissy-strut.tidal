  stack[
    slowcat[
       perc "[x ~ ~ x][~ x ~ ~][~ x ~ x][x ~ x ~]",
       perc "[x ~ ~ x][~ ~ ~ x][~ x ~ x][x ~ x ~]"
       ] # perc "bd",
    slowcat[
      perc "[~ ~ ~ ~][x ~ ~ x][~ x x ~][~ ~ ~ ~]",
      perc "[~ ~ x ~][~ x x ~][x x ~ ~][~ ~ ~ ~]"
      ] # perc "sn",
    perc "[~ ~ ~ ~][~ ~ ~ ~][~ ~ ~ ~][x ~ x ~]" # perc "oh"
    ]
