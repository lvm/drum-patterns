-- Clyde Stubblefield
-- Funky drummer
-- 1970

bps (101/120)

drums $
  stack[
    slowcat[
       perc "[x ~ x ~][~ ~ ~ ~][~ ~ x ~][~ x ~ ~]" # perc "bd",
       perc "[x ~ x ~][~ ~ ~ ~][~ ~ x ~][~ x ~ ~]" # perc "bd" 
    ],
    slowcat[
      perc "[~ ~ ~ ~][x ~ ~ x][~ x ~ x][x ~ ~ x]" # perc "sn",
      perc "[~ ~ ~ ~][x ~ ~ x][~ x ~ x][x ~ ~ x]" # perc "sn"
      ],
    perc $ preplace (1,1) ("x(16,16)" :: Pattern String) "ch"
    ]
